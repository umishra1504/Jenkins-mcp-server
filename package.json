{
	"name": "jenkins-mcp-server",
	"version": "1.0.0",
	"description": "A comprehensive Jenkins Model Context Protocol (MCP) server for managing builds, jobs, artifacts, and queue operations",
	"main": "src/index.js",
	"type": "module",
	"bin": {
		"jenkins-mcp": "src/index.js"
	},
	"scripts": {
		"start": "node src/index.js",
		"dev": "nodemon src/index.js",
		"test": "node --test",
